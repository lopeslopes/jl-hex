\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
\vec{a}_{1}=\begin{pmatrix}a\\
0
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\vec{a}_{2}=\begin{pmatrix}\nicefrac{a}{2}\\
\nicefrac{a\sqrt{3}}{2}
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\vec{d}=\frac{1}{3}\left(\vec{a}_{1}+\vec{a}_{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\vec{r}_{A}=n\vec{a}_{1}+m\vec{a}_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\vec{r}_{B}=i\vec{a}_{1}+j\vec{a}_{2}+\vec{d}=i\vec{a}_{1}+j\vec{a}_{2}+\frac{1}{3}\vec{a}_{1}+\frac{1}{3}\vec{a}_{2}=\left(\frac{3i+1}{3}\right)\vec{a}_{1}+\left(\frac{3j+1}{3}\right)\vec{a}_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R=\begin{pmatrix}\cos\theta & -\sin\theta\\
\sin\theta & \cos\theta
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R\vec{r}_{B}=\vec{r}_{A}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{pmatrix}\cos\theta & -\sin\theta\\
\sin\theta & \cos\theta
\end{pmatrix}\left[\left(\frac{3i+1}{3}\right)\vec{a}_{1}+\left(\frac{3j+1}{3}\right)\vec{a}_{2}\right]=n\vec{a}_{1}+m\vec{a}_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{pmatrix}\cos\theta & -\sin\theta\\
\sin\theta & \cos\theta
\end{pmatrix}\left[\begin{pmatrix}a\frac{3i+1}{3}\\
0
\end{pmatrix}+\begin{pmatrix}\nicefrac{a}{2}\frac{3j+1}{3}\\
\nicefrac{a\sqrt{3}}{2}\frac{3j+1}{3}
\end{pmatrix}\right]=\begin{pmatrix}na\\
0
\end{pmatrix}+\begin{pmatrix}m\nicefrac{a}{2}\\
m\nicefrac{a\sqrt{3}}{2}
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{pmatrix}\cos\theta & -\sin\theta\\
\sin\theta & \cos\theta
\end{pmatrix}\begin{pmatrix}a\frac{3i+1}{3}+\frac{a}{2}\frac{3j+1}{3}\\
a\frac{\sqrt{3}}{2}\frac{3j+1}{3}
\end{pmatrix}=\begin{pmatrix}na+m\frac{a}{2}\\
m\frac{a\sqrt{3}}{2}
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{pmatrix}\cos\theta & -\sin\theta\\
\sin\theta & \cos\theta
\end{pmatrix}\begin{pmatrix}a\frac{3i+1}{3}+\frac{a}{2}\frac{3j+1}{3}\\
a\frac{\sqrt{3}}{2}\frac{3j+1}{3}
\end{pmatrix}=\begin{pmatrix}na+m\frac{a}{2}\\
m\frac{a\sqrt{3}}{2}
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
Primeira componente:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cos\theta\left[a\frac{3i+1}{3}+\frac{a}{2}\frac{3j+1}{3}\right]-\sin\theta\left[a\frac{\sqrt{3}}{2}\frac{3j+1}{3}\right]=na+m\frac{a}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{3i+1}{3}\cos\theta+\frac{3j+1}{6}\cos\theta-\sqrt{3}\frac{3j+1}{6}\sin\theta=n+\frac{m}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Segunda componente:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin\theta\left[a\frac{3i+1}{3}+\frac{a}{2}\frac{3j+1}{3}\right]+\cos\theta\left[a\frac{\sqrt{3}}{2}\frac{3j+1}{3}\right]=m\frac{a\sqrt{3}}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{3i+1}{3}\sin\theta+\frac{3j+1}{6}\sin\theta+\sqrt{3}\frac{3j+1}{6}\cos\theta=m\frac{\sqrt{3}}{2}
\]

\end_inset


\end_layout

